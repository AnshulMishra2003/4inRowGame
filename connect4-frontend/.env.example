# React App Environment Variables
# Note: Only variables prefixed with REACT_APP_ will be accessible in the React app

# Backend API URL
REACT_APP_API_URL=http://localhost:3001

# Socket.IO URL (if different from API URL)
REACT_APP_SOCKET_URL=http://localhost:3001

# Environment
REACT_APP_ENV=development

# Debug mode
REACT_APP_DEBUG=true

# Game Configuration
REACT_APP_GAME_TIMEOUT=30000
REACT_APP_MAX_PLAYERS=2

# Add your custom environment variables here
# Remember: REACT_APP_ prefix is required for frontend variables
